{
  "name": "ai-warp-cli",
  "version": "0.1.0",
  "description": "AI-powered Warp CLI with Supabase integration and Ollama for local LLM inference",
  "main": "dist/cli.js",
  "bin": {
    "warp": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "start:api": "ts-node src/api/server.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "local:start": "npm run docker:up && npm run start:api",
    "local:stop": "npm run docker:down"
  },
  "keywords": [
    "cli",
    "ai",
    "llm",
    "ollama",
    "vector-database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "body-parser": "^1.20.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
